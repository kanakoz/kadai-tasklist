<%= form_for(@list) do |f| %>

　<% if list.errors.any? %>
　<div id="error_explanation">
　  <ul>
　    <% list.errors.full_messages.each do |message| %>
　      <li><%= message %></li>
　    <% end %>
　  </ul>
　</div>
　<% end %>
　

  <%= f.label :content, 'タスク' %>
  <%= f.text_field :content %>
  
  <%= f.submit '投稿' %>
<% end %>